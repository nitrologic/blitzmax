Print "ATan(100)= " + ATan(100)
Print "ATan2(3, 4)= " + ATan2(3, 4)
Print "Tanh(33) = " + Tanh(33)
Print "Exp(15) = " + Exp(15)
Print "Log(20) = " + Log(20)
